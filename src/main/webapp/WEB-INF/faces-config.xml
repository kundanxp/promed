<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <el-resolver>com.ea.promed.beans.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>doctor</managed-bean-name>
        <managed-bean-class>com.ea.promed.beans.DoctorController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>com.ea.promed.entities.Doctor</converter-for-class>
        <converter-class>com.ea.promed.beans.DoctorConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>doctor_create</from-outcome>
            <to-view-id>/dashboard/doctor/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>doctor_list</from-outcome>
            <to-view-id>/dashboard/doctor/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>doctor_edit</from-outcome>
            <to-view-id>/dashboard/doctor/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>doctor_detail</from-outcome>
            <to-view-id>/dashboard/doctor/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>doctorJpa</managed-bean-name>
        <managed-bean-class>com.ea.promed.facades.DoctorFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>nurse</managed-bean-name>
        <managed-bean-class>com.ea.promed.beans.NurseController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>com.ea.promed.entities.Nurse</converter-for-class>
        <converter-class>com.ea.promed.beans.NurseConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>nurse_create</from-outcome>
            <to-view-id>/dashboard/nurse/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>nurse_list</from-outcome>
            <to-view-id>/dashboard/nurse/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>nurse_edit</from-outcome>
            <to-view-id>/dashboard/nurse/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>nurse_detail</from-outcome>
            <to-view-id>/dashboard/nurse/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <el-resolver>com.ea.promed.facades.util.JsfCrudELResolver</el-resolver>
    </application>
    <managed-bean>
        <managed-bean-name>nurseJpa</managed-bean-name>
        <managed-bean-class>com.ea.promed.facades.NurseFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
